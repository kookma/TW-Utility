\define show-macro(tiddler)
<$set name=selected-tiddler value="$tiddler$" emptyValue=<<currentTiddler>> >
<$macrocall $name='find'
  text={{{[<selected-tiddler>get[text]]}}}
  begin="\define"
  end="\end"
  output="disp-macro-contents"/>
</$set>
\end